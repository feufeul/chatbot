package be.formation.beans;

public class chatUser{

	/**
	 * Chatter's Nickname
	 */
	private String name;
	
	/**
	 * chatUser's constructor
	 * @param name used by chatter
	 */
	chatUser(String name) {

		this.setName(name);
	}

	/**
	 * Getter for the chatter's name
	 * @return chatter's name
	 */
	public String getName() {
		return name;
	}
	

	
}
